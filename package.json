{
  "name": "n8n-nodes-groupme",
  "version": "0.1.0",
  "description": "n8n community nodes for interacting with the GroupMe API",
  "license": "MIT",
  "homepage": "",
  "keywords": ["n8n-community-node-package", "groupme", "n8n"],
  "author": { "name": "Gwa Shark", "email": "gwabiz@proton.me" },
  "repository": {
    "type": "git",
    "url": "https://github.com/v0ltedNoders/n8n-nodes-groupme.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": ["dist", "package.json", "nodes", "credentials", "README.md"],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": ["dist/credentials/GroupmeApi.credentials.js"],
    "nodes": [
      "dist/nodes/Groupme/Groupme.node.js",
      "dist/nodes/Groupme/GroupmeTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": { "n8n-workflow": "*" }
}
